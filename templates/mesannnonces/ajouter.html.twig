{% extends 'base.html.twig' %}

{% block title %}Ajouter Annonce{% endblock %}

{% block body %}
<h2>Ajouter une annonce</h2>
<div class="card">
    <div class="card-header">S'inscrire</div>
    <div class="card-body">
        {{form_start(form)}}
        {{form_widget(form)}}
        <input type="submit" class="btn btn-success" value="Enregistrer">
        {{form_end(form)}}
    </div>
</div>
{% endblock %}